<Window x:Class="AutoTapGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoTapGUI"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="AutoTap" Height="700" Width="670" Closing="Window_Closing" StateChanged="Window_StateChanged" Loaded="Window_Loaded" Icon="Asset/Phone.ico">

    <DockPanel LastChildFill="True">
        <GroupBox Header="基本操作" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" DockPanel.Dock="top">
            <StackPanel Orientation="Vertical" Margin="5">
                <StackPanel Orientation="Horizontal" Margin="5">
                    <Label>端末設定を選択</Label>
                    <ComboBox x:Name="comboBoxSelectConfig" Margin="0,0,0,0" SelectionChanged="comboBoxSelectConfig_SelectionChanged" MinWidth="80"/>
                    <Label>動作モード</Label>
                    <ComboBox x:Name="comboBoxWorkingMode" Margin="0,0,0,0" SelectionChanged="comboBoxWorkingMode_SelectionChanged" MinWidth="80"/>
                    <Label>次の動作モード</Label>
                    <ComboBox x:Name="comboBoxNextMode" Margin="0,0,0,0" SelectionChanged="comboBoxNextMode_SelectionChanged" MinWidth="80"/>
                </StackPanel>
                <DockPanel LastChildFill="True" Margin="5">
                    <Button x:Name="buttonReset" Content="端末設定リセット" HorizontalAlignment="Right" Width="100" Margin="5,0,0,0" Height="23" DockPanel.Dock="Right" Click="buttonReset_Click"/>
                    <Button x:Name="buttonStart" Content="開始" HorizontalAlignment="Left" Width="60" Margin="5,0,0,0" Height="23" Click="buttonStart_Click" DockPanel.Dock="Left"/>
                    <Button x:Name="buttonStop" Content="停止" HorizontalAlignment="Left" Width="60" Margin="5,0,0,0" Height="23" Click="buttonStop_Click" DockPanel.Dock="Left"/>
                    <Button x:Name="buttonQuit" Content="終了" HorizontalAlignment="Left" Width="60" Margin="5,0,0,0" Height="23" Click="buttonQuit_Click" />
                </DockPanel>
            </StackPanel>
        </GroupBox>
        <TabControl x:Name="tabControl" Margin="10,10,10,10">
            <TabItem Header="ログ">
                <DockPanel LastChildFill="True">
                    <TextBox x:Name="textBoxLog" IsReadOnly="True" ScrollViewer.VerticalScrollBarVisibility="Auto" TextChanged="textBoxLog_TextChanged"/>
                </DockPanel>
            </TabItem>
