                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5">
                                    <Label Content="レーダー試行判断回数:"  ToolTip="「レーダー試行判断時間」個のにこの回数分レーダーを使用した場合、「レーダー再試行インターバル」分待ちます。"/>
                                    <TextBox x:Name="RadarCheckInTryNum" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="回" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5">
                                    <Label Content="レーダー試行判断時間:" ToolTip="この時間内に「レーダー試行判断回数」個のレーダーを使用した場合、「レーダー再試行インターバル」分待ちます。"/>
                                    <TextBox x:Name="RadarCheckInTryTime" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="ms" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5">
                                    <Label Content="レーダー再試行インターバル最小値:" ToolTip="「レーダー試行判断時間」内に「レーダー試行判断回数」個のレーダーを使用した場合、この時間分待つ最小値です。" />
                                    <TextBox x:Name="RadarCheckInTryIntervalMin" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="ms" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5">
                                    <Label Content="レーダー再試行インターバル最大値:" ToolTip="「レーダー試行判断時間」内に「レーダー試行判断回数」個のレーダーを使用した場合、この時間分待つ最大値です。" />
                                    <TextBox x:Name="RadarCheckInTryIntervalMax" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="ms" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5">
                                    <Label Content="使用していいレーダーの数:" />
                                    <TextBox x:Name="RadarUseableNum" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="回" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5">
                                    <Label Content="「Tough」モード時の回復時間最小値:" />
                                    <TextBox x:Name="ToughRecoveryTimeMin" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="ms" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5">
                                    <Label Content="「Tough」モード時の回復時間最大値:" />
                                    <TextBox x:Name="ToughRecoveryTimeMax" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="ms" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5">
                                    <Label Content="「Tough」モード時の生存チェックインターバル最小値:" />
                                    <TextBox x:Name="ToughCheckAliveIntervalMin" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="ms" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5">
                                    <Label Content="「Tough」モード時の生存チェックインターバル最大値:" />
                                    <TextBox x:Name="ToughCheckAliveIntervalMax" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="ms" />
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox Header="HP切り出しの範囲" Margin="10">
                            <StackPanel Orientation="Vertical">
                                <StackPanel Orientation="Horizontal" Margin="10,10,10,5">
                                    <Label Content="左上の座標" />
                                    <Label Content="X:" />
                                    <TextBox x:Name="ScreenShotTrimX" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="Y:" />
                                    <TextBox x:Name="ScreenShotTrimY" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,10">
                                    <Label Content="画像サイズ" />
                                    <Label Content="幅:" />
                                    <TextBox x:Name="ScreenShotTrimWidth" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="高さ:" />
                                    <TextBox x:Name="ScreenShotTrimHeight" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox Header="画像" Margin="10">
                            <StackPanel Orientation="Vertical">
                                <StackPanel Orientation="Horizontal" Margin="10,10,10,5">
                                    <Label Content="トリミング画像の保存パス:" />
                                    <TextBox x:Name="TrimImageLocalPath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="OCRしたテキストの保存パス:" />
                                    <TextBox x:Name="OcrTextLocalPath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="Link比較画像の保存パス:" />
                                    <TextBox x:Name="ComparedLinkImagePath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="Error比較画像の保存パス:" />
                                    <TextBox x:Name="ComparedErrorImagePath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="あいさつ比較画像の保存パス:" />
                                    <TextBox x:Name="ComparedGreetingImagePath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="ロード中比較画像Aの保存パス:" />
                                    <TextBox x:Name="ComparedLoadingImagePathA" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="ロード中比較画像Bの保存パス:" />
                                    <TextBox x:Name="ComparedLoadingImagePathB" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="メンテナンス比較画像の保存パス:" />
                                    <TextBox x:Name="ComparedMaintenanceImagePath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="起動時比較画像の保存パス:" />
                                    <TextBox x:Name="ComparedOpeningImagePath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="タイトル比較画像の保存パス:" />
                                    <TextBox x:Name="ComparedTitleImagePath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="タイムライン比較画像の保存パス:" />
                                    <TextBox x:Name="ComparedTimelineImagePath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="スクリーンショットの端末側保存パス:" />
                                    <TextBox x:Name="ScreenShotRemotePath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,10">
                                    <Label Content="スクリーンショットのPC側保存パス:" />
                                    <TextBox x:Name="ScreenShotLocalPath" Height="23" Width="200" TextChanged="Config_TextChanged" />
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox Header="閾値" Margin="10">
                            <StackPanel Orientation="Vertical">
                                <StackPanel Orientation="Horizontal" Margin="10,10,10,5">
                                    <Label Content="パターンマッチングの閾値(0~1):" />
                                    <TextBox x:Name="PatternMatchingThreshold" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="バッテリー十分ある閾値:" />
                                    <TextBox x:Name="BatteryEnoughThreshold" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="バッテリー充電完了閾値:" />
                                    <TextBox x:Name="BatteryMaxThreshold" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,10">
                                    <Label Content="回復しない閾値:" />
                                    <TextBox x:Name="NoRecoverThreshold" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox Header="アンロック" Margin="10">
                            <StackPanel Orientation="Vertical">
                                <StackPanel Orientation="Horizontal" Margin="10,10,10,5">
                                    <Label Content="スワイプ開始座標" />
                                    <Label Content="X:" />
                                    <TextBox x:Name="UnlockXStart" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="Y:" />
                                    <TextBox x:Name="UnlockYStart" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="スワイプ終了座標" />
                                    <Label Content="X:" />
                                    <TextBox x:Name="UnlockXEnd" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="Y:" />
                                    <TextBox x:Name="UnlockYEnd" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="スワイプ時間:" />
                                    <TextBox x:Name="UnlockDuration" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                    <Label Content="ms:" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,5">
                                    <Label Content="アンロックの種類(Pin/Password/None):" />
                                    <TextBox x:Name="UnlockType" Height="23" Width="100" TextChanged="Config_TextChanged" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="10,5,10,10">
                                    <Label Content="PIN/Password:" />
                                    <PasswordBox x:Name="UnlockPassword" Height="23" Width="100" PasswordChanged="Config_TextChanged" />
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <Button x:Name="buttonSave" Content="Save" Margin="10" Width="75" HorizontalAlignment="Right" Click="buttonSave_Click" />
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
            <TabItem Header="動作設定">
                <StackPanel Orientation="Vertical">
                    <CheckBox x:Name="isStartMinimize" Content="開始時に最小化" Margin="10,10,10,10" VerticalAlignment="Center" Loaded="isStartMinimize_Loaded" Checked="isStartMinimize_Checked" Unchecked="isStartMinimize_Checked"/>
                    <CheckBox x:Name="isShowSleepDetail" Content="詳細なスリープをログに表示" Margin="10,10,10,10" VerticalAlignment="Center" Loaded="isShowSleepDetail_Loaded" Checked="isShowSleepDetail_Checked" Unchecked="isShowSleepDetail_Checked"/>
                    <CheckBox x:Name="isOutputLog" Content="ログをファイルに出力" Margin="10,10,10,10" VerticalAlignment="Center" Loaded="isOutputLog_Loaded" Checked="isOutputLog_Checked" Unchecked="isOutputLog_Checked"/>
                    <StackPanel Orientation="Horizontal" Margin="10,10,10,10">
                        <CheckBox x:Name="isStartTimer" Content="開始タイマー" VerticalAlignment="Center" Loaded="isStartTimer_Loaded" Checked="isStartTimer_Checked" Unchecked="isStartTimer_Checked"/>
                        <WindowsFormsHost Margin="10,0,0,0" MinWidth="230" x:Name="wfhStart">
                            <wf:DateTimePicker ShowUpDown="True" Format="Custom" CustomFormat="yyyy年MM月dd日(dddd)HH時mm分ss秒" />
                        </WindowsFormsHost>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="10,10,10,0">
                        <CheckBox x:Name="isStopTimer" Content="終了タイマー" VerticalAlignment="Center" Loaded="isStopTimer_Loaded" Checked="isStopTimer_Checked" Unchecked="isStopTimer_Checked"/>
                        <WindowsFormsHost Margin="10,0,0,0" MinWidth="230" x:Name="wfhStop">
                            <wf:DateTimePicker ShowUpDown="True" Format="Custom" CustomFormat="yyyy年MM月dd日(dddd)HH時mm分ss秒" />
                        </WindowsFormsHost>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="10,10,10,0">
                        <Label Content="使用するバッテリー:" Margin="0,0,5,0"/>
                        <CheckBox x:Name="isUseBattery50" Content="50" VerticalAlignment="Center" Loaded="isUseBattery50_Loaded" Checked="isUseBattery50_Checked" Unchecked="isUseBattery50_Checked" Margin="0,0,10,0"/>
                        <CheckBox x:Name="isUseBattery150" Content="150" VerticalAlignment="Center" Loaded="isUseBattery150_Loaded" Checked="isUseBattery150_Checked" Unchecked="isUseBattery150_Checked" Margin="0,0,10,0"/>
                        <CheckBox x:Name="isUseBattery300" Content="300" VerticalAlignment="Center" Loaded="isUseBattery300_Loaded" Checked="isUseBattery300_Checked" Unchecked="isUseBattery300_Checked" Margin="0,0,10,0"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="10,10,10,10">
                        <Label Content="使用するレーダー:　上から" Margin="0,0,0,0"/>
                        <ComboBox x:Name="comboBoxSelectRadarOrder" Margin="0,0,0,0" SelectionChanged="comboBoxSelectRadarOrder_SelectionChanged"/>
                        <Label Content="個下のレーダー（0が最も上、最大7まで）" Margin="0,0,0,0"/>
                    </StackPanel>
                </StackPanel>
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
